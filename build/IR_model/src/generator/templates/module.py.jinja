"""{{ module_name }} module for {{ module_description }}.
Version: {{ version }}
Compatible with Core PRD v{{ core_prd_version }}
"""
from typing import Dict, List, Any, Optional
{% for import in imports %}
{{ import }}
{% endfor %}

{% if logger_import %}
from utils.logging import setup_logging

logger = setup_logging()
{% endif %}

{% for interface in interfaces %}
class {{ interface.name }}:
    """{{ interface.description }}"""
    
    {% for method in interface.methods %}
    def {{ method.name }}(self{% if method.parameters %}, {% endif %}
        {%- for param in method.parameters -%}
        {{ param.name }}: {{ param.type }}{% if not loop.last %}, {% endif %}
        {%- endfor -%}) -> {{ method.return_type }}:
        """{{ method.description }}
        
        Args:
            {% for param in method.parameters %}
            {{ param.name }}: {{ param.description }}
            {% endfor %}
            
        Returns:
            {{ method.return_description }}
            
        {% if method.raises %}
        Raises:
            {% for error in method.raises %}
            {{ error.type }}: {{ error.description }}
            {% endfor %}
        {% endif %}
        """
        {{ method.body | indent(8) }}
    {% endfor %}
{% endfor %}

{% for function in functions %}
def {{ function.name }}({% if function.parameters %}
    {%- for param in function.parameters -%}
    {{ param.name }}: {{ param.type }}{% if not loop.last %}, {% endif %}
    {%- endfor -%}
{% endif %}) -> {{ function.return_type }}:
    """{{ function.description }}
    
    Args:
        {% for param in function.parameters %}
        {{ param.name }}: {{ param.description }}
        {% endfor %}
        
    Returns:
        {{ function.return_description }}
        
    {% if function.raises %}
    Raises:
        {% for error in function.raises %}
        {{ error.type }}: {{ error.description }}
        {% endfor %}
    {% endif %}
    """
    {{ function.body | indent(4) }}
{% endfor %}
